  "drops": {
    "description": "An array of monster drop objects.",
    "type": "list",
    "required": true,
    "nullable": false,
    "schema": {
      "type": "dict",
      "required": false,
      "schema": {
        "id": {
          "description": "The ID number of the item drop.",
          "type": "integer",
          "required": true,
          "nullable": false
        },
        "name": {
          "description": "The name of the item drop.",
          "type": "string",
          "required": true,
          "nullable": false
        },
        "members": {
          "description": "If the drop is a members-only item.",
          "type": "boolean",
          "required": true,
          "nullable": false
        },
        "quantity": {
          "description": "The quantity of the item drop (integer, comma-separated or range).",
          "type": "string",
          "required": true,
          "nullable": true,
          "regex": "^((([0-9]+)|([0-9]+-[0-9]+))(,(([0-9]+)|([0-9]+-[0-9]+)))*)$"

        },
        "noted": {
          "description": "If the item drop is noted, or not.",
          "type": "boolean",
          "required": true,
          "nullable": false
        },
        "rarity": {
          "description": "The rarity of the item drop (as a float out of 1.0).",
          "type": "float",
          "required": true,
          "nullable": true
        },
        "drop_requirements": {
          "description": "If there are any requirements to getting the specific drop.",
          "type": "string",
          "required": true,
          "nullable": true,
          "allowed": [
            "krystilia-task-only",
            "superior-only",
            "catacombs-only",
            "konar-task-only",
            "treasure-trails-only",
